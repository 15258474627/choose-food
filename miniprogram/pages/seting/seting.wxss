.page-section{
  padding: 10px;
}
.weui-input{
  border: 1px solid #eee;
  padding: 4px;
  margin-top: 4px;
}

.btn-area > button{
  width: 30%;
  margin: 0 10%;
  float: left;
  font-size: 14px;
}
.btn-area{
  overflow: hidden;
  margin-bottom: 20px;
}
.line{
  width: 100%;
  border-top: 1px dotted #eee;
}

.label-1{
    margin-bottom: 15px;
    position:relative;
}
.label-1__text {
    display: inline-block;
    vertical-align: middle;
}

.label-1__icon {
    position: relative;
    margin-right: 10px;
    display: inline-block;
    vertical-align: middle;
    width: 18px;
    height: 18px;
    background: #fcfff4;
}

.label-1__icon-checked {
    position: absolute;
    top: 3px;
    left: 3px;
    width: 12px;
    height: 12px;
    background: #1aad19;
}
.delete{
  position: absolute;
  right: 10px;
  top: 0;
}